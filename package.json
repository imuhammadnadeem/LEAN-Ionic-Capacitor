{
  "name": "lean-ionic-capacitor",
  "version": "1.0.14",
  "description": "Capacitor plugin for integrating Lean Technologies into Ionic apps with native iOS/Android SDK support and Web SDK fallback.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "Frameworks/",
    "ios/Sources",
    "ios/Tests",
    "Package.swift",
    "LeanIonicCapacitor.podspec"
  ],
  "author": "Muhammad Nadeem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imuhammadnadeem/LEAN-Ionic-Capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/imuhammadnadeem/LEAN-Ionic-Capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "capacitor-plugin",
    "ionic",
    "plugin",
    "native",
    "lean",
    "lean-technologies",
    "lean-link",
    "lean-sdk",
    "open-banking",
    "open-finance",
    "fintech",
    "payments",
    "banking",
    "financial-data",
    "account-aggregation",
    "ios",
    "android",
    "web"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "xcodebuild -scheme LeanIonicCapacitor -destination 'generic/platform=iOS' -configuration Debug build",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "check": "npm run check:ios && npm run check:android && npm run check:web",
    "check:ios": "xcodebuild -scheme LeanIonicCapacitor -destination 'generic/platform=iOS' -configuration Debug build",
    "check:ios:simulator": "xcodebuild -scheme LeanIonicCapacitor -destination 'platform=iOS Simulator,name=iPhone 16' -configuration Debug build",
    "check:android": "cd android && ./gradlew clean build test && cd ..",
    "check:web": "npm run build && cd example-app && npm install && npm run build",
    "test:web": "npm run build && node --test test/web/*.test.mjs",
    "test:android": "cd android && ./gradlew test && cd ..",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"{src,android/src}/**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java --ignore-path .prettierignore",
    "swiftlint": "node-swiftlint lint ios Package.swift",
    "docgen": "docgen --api Lean --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/docgen": "^0.3.1",
    "@capacitor/ios": "^8.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.7",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "swiftlint": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  },
  "peerDependencies": {
    "@capacitor/core": "^6.0.0 || ^7.0.0 || ^8.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
